@model SampleASPCore.Models.ViewModels.RestaurantViewModel

@{
    ViewData["Title"] = "GetRestoWithCust";
}

<h1>Get Resto With Cust</h1>

<ul>
    @foreach(var resto in Model.Restaurant)
    {
        <li>@resto.Name</li>
    }
</ul>

<p>Customer Name: @Model.Customer.CustName</p>

<address>
    <strong>Support:</strong><email mail-to="info">Info</email><br />
    <strong>Marketing:</strong><email mail-to="marketing">Marketing</email>
</address>
